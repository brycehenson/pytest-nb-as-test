   "source": [
    "from multiprocessing import Pool\n",
    "\n",
    "\n",
    "def fit_for_duration(duration):\n",
    "    return duration\n",
    "\n",
    "\n",
    "durations = [0.1, 0.2]\n",
    "with Pool(processes=2) as pool:\n",
    "    list(pool.imap(fit_for_duration, durations))"
   ]
  }
