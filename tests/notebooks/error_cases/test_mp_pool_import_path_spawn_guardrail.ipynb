   "source": [
    "import multiprocessing as mp\n",
    "from multiprocessing.pool import Pool\n",
    "\n",
    "\n",
    "def f(x):\n",
    "    return x + 1\n",
    "\n",
    "\n",
    "ctx = mp.get_context(\"spawn\")\n",
    "with Pool(processes=2, context=ctx) as pool:\n",
    "    out = list(pool.imap(f, [1, 2, 3]))\n",
    "\n",
    "assert out == [2, 3, 4]"
   ]
  }
