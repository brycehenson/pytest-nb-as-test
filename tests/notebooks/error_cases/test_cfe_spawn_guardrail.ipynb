   "source": [
    "import multiprocessing as mp\n",
    "from concurrent.futures import ProcessPoolExecutor\n",
    "\n",
    "\n",
    "def f(x):\n",
    "    return x + 1\n",
    "\n",
    "\n",
    "ctx = mp.get_context(\"spawn\")\n",
    "with ProcessPoolExecutor(max_workers=2, mp_context=ctx) as ex:\n",
    "    out = list(ex.map(f, [1, 2, 3]))\n",
    "\n",
    "assert out == [2, 3, 4]"
   ]
  }
