   "source": [
    "import multiprocessing as mp\n",
    "\n",
    "\n",
    "def f(x):\n",
    "    return x + 1\n",
    "\n",
    "\n",
    "ctx = mp.get_context(\"spawn\")\n",
    "process = ctx.Process(target=f, args=(1,))\n",
    "process.start()\n",
    "process.join()\n",
    "assert process.exitcode == 0"
   ]
  }
