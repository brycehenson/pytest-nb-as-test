   "source": [
    "# pytest-nb-as-test: cell-timeout-seconds=5\n",
    "\n",
    "import multiprocessing as mp\n",
    "\n",
    "\n",
    "def f(x):\n",
    "    return x + 1\n",
    "\n",
    "\n",
    "ctx = mp.get_context(\"fork\")\n",
    "with ctx.Pool(processes=2) as pool:\n",
    "    out = list(pool.imap(f, [1, 2, 3]))\n",
    "\n",
    "assert out == [2, 3, 4]"
   ]
  }
